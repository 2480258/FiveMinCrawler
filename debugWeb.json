{
  "requestFormat": {
    "engines": [
      {
        "requesterEngineName": "Default",
        "type": "Default",
        "requesters": [
          {
            "userAgent": "TESTTESTTEST"
          }
        ]
      }
    ],
    "cookiePolicies": []
  },
  "parseFormat": {
    "bookName": "Debug",
    "globalCondition": {
      "uriRegex": "127.0.0.1:30001"
    },
    "pages": [
      {
        "pageName": "home",
        "condition": {
          "uriRegex": "127.0.0.1:30001/home"
        },
        "internalAttributes": [],
        "linkAttributes": [
          {
            "attributeName": "links",
            "queryStr": "body > a",
            "uriRegex": "\\/"
          }
        ],
        "externalAttributes": [
          {
            "attributeName": "testImages",
            "queryStr": "body > div",
            "uriRegex": ".png"
          }
        ],
        "targetContainer": {
          "workingSetMode": "Enabled"
        },
        "tag": [
          {
            "name": "uri",
            "tagRegex": "([a-z]+)",
            "isAlias": false
          }
        ],
        "targetRequesterEngine": {
          "targetRequester": "Default"
        }
      },
      {
        "pageName": "users",
        "condition": {
          "uriRegex": "127.0.0.1:30001/users"
        },
        "internalAttributes": [
          {
            "attributeName": "C1234567890",
            "queryStr": "body > h1",
            "parseMode": "TEXT_CONTENT"
          }
        ],
        "linkAttributes": [],
        "externalAttributes": [],
        "targetContainer": {
          "workingSetMode": "Disabled"
        },
        "tag": [
          {
            "name": "uri",
            "tagRegex": "([a-z]+)",
            "isAlias": false
          }
        ],
        "targetRequesterEngine": {
          "targetRequester": "Default"
        }
      },
      {
        "pageName": "about",
        "condition": {
