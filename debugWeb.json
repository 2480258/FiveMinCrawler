  "exportFormat": {
    "bookName": "Debug",
    "pages": [
      {
        "pageName": "home",
        "targetAttributeName": [
          "testImages"
        ],
        "adapter": {
          "mode": "Binary",
          "fileNameTagExp": "&(inc).&(ext)"
        }
      },
      {
        "pageName": "about",
        "targetAttributeName": [
          "A1234567890"
        ],
        "adapter": {
          "mode": "Json",
          "fileNameTagExp": "&(lastseg)\\about.json"
        }
      },
      {
        "pageName": "users",
        "targetAttributeName": [
          "C1234567890"
        ],
